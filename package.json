    "prepack": "build-esm"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^2.2.0",
    "build-esm": "^4.2.1",
    "flow-bin": "^0.104.0",
    "flow-typed": "^2.6.1",
    "overdub": "^2.4.0",
    "semantic-release": "^15.13.19",
    "standard-esnext": "^5.3.3"
  },
  "ava": {
    "require": "overdub/register",
    "babel": {
      "testOptions": {
        "extends": "overdub/babel"
      }
    }
